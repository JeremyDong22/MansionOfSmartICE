{
  "buildCommand": "cd mansion-of-smartice && npm run build",
  "outputDirectory": "mansion-of-smartice/.next",
  "installCommand": "cd mansion-of-smartice && npm install",
  "framework": "nextjs",
  "regions": ["sin1"],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/mansion-of-smartice/$1"
    }
  ]
}